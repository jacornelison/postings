<script type="text/javascript">
    function update_params(newparams) {
        for (key of Object.keys(param)) {
            if (newparams[key] !== undefined) {
                param[key] = newparams[key];
            }
        }
        plupdate();
    }
    // Parameters that the user can set using the
    // pager buttons.  These are the options that will
    // come up by default when the page first loads.
    // You can define whatever parameters you like,
    //   param.something='value';
    var param = {};
    param.dp='1102';
    param.jack='0';
    param.lmax='200';
    param.figtype='c6';
    param.pb='_pureB_cm_overfreq';
    param.bpwf='';
    param.simrd='';
    param.cross1='1';
    param.cross2='3';
    param.version='B2018';
    param.year = '2018';
    param.sim = '499';


    function make_fignames() {
        fig.fig1={};
	fig.chitab={};
        var rot = '';
        var daughter = '';
        var bpwf = param.bpwf;

        // Deal with choosing cross spectra consistently.
        var c1 = parseInt(param.cross1);
        var c2 = parseInt(param.cross2);
        var c11 = Math.min(c1, c2);
        var c22 = Math.max(c1, c2);

        // Define map names for all possible crosses
        var exmap = {};
        // Allow versions to modify the path and map names used:
        switch (param.version) {
          case 'K2014':
            vs = '1351_d';
            daughter = 'd';

            exmap['1'] = 'K2014_100';
            exmap['2'] = 'K2014_150';
            exmap['3'] = 'null';

            break;

          case 'K2015':
            vs = '1351';
            daughter = 'e';

            exmap['1'] = 'K2015_100';
            exmap['2'] = 'K2015_150';
            exmap['3'] = 'K2015_220';

            break;

          case 'B2016':
            vs = '3513';
            daughter = 'f';
            rot = '_norot';
            break;

	  case 'B2017':
	    vs = '3513';
	    daughter = 'g';
            rot = '_norot';
	    break;

          case 'B2018':
            vs = '3513';
            daughter = 'h';
            rot = '_norot';
            break;

          case 'K2015_norot':
            vs = '1351_norot';
            daughter = 'e';

            // This posting is using unrotated final files, which only
            // includes an annotation for jack0
            if (param.jack == '0') {
                rot = '_norot';
            }

            exmap['1'] = 'K2015_100';
            exmap['2'] = 'K2015_150';
            exmap['3'] = 'K2015_220';

            break;
        }

        // Build map extention to use based on choices.
        exname = exmap[c11] + 'x' + exmap[c22];

        foc='final';
        if (param.figtype[0]=='c') {
            figtype=param.figtype[1];
            foc='chi2';
        } else {
            figtype=param.figtype;
        }

        fig.fig1.fname = 'final/' + vs + '/real_' + daughter
            + '_filtp3_weight3_gs_dp' + param.dp
            + '_jack' + param.jack + '1'
            + param.pb + rot + bpwf + param.simrd + '_' + foc + '_'
            + param.lmax + '_' + figtype + '.png';
	
	fig.chitab.fname = param.year + '/pte_dist_B' + param.year + '_202008.png';    

    }

    set_figname_fun(make_fignames);

    // Automatically colorize the buttons as they're pressed
    //   set_autocolor(1,on_color,off_color)
    //   set_autocolor(0) to turn off this feature
    set_autocolor(1,'Red','LightBlue');

    // Check if the user is looking for particular settings
    get_query_words();

    // Setup for automatic pager button writer
    pager.dp=['Deproj',
        '1100 rg+dp','1100',
        '1102 rg+dp + meas diff ellipt','1102'
        ];
    pager.pb=['Estimator',
        'normal','_cm_overfreq',
        'kendrick','_pureB_cm_overfreq',
        // 'matrix','_pureB_matrix_cm_overfreq'
        ];
    pager.bpwf=['BPWF',
        'normal','',
        // 'direct','_directbpwf'
        ];
    pager.figtype=['Figure',
        // Row 1
        'raw signal+noise','1',
        '..after noise debias','2',
        // Row 2
        'Filter/beam suppression factors','4',
        '..after filter/beam correction','6',
        // Row 3
        '','',
        '','',
        // Row 4
        'spectra fixed y-axis','c1',
        'spectra free y-axis','c6',
        // Row 5
        'bp devs','c2',
        'chi2','c3',
        // Row 6
        'sum bp devs','c4',
        'uncertainties','c5'
        ];
    pager.jack=['Jack',
        'none','0',
        'dk','1',
        'scan dir','2',
        '1st/2nd half weights','3',
        'tile','4',
        'phase (az)','5',
        'mux col','6',
        'alt dk','7',
        'mux row','8',
        'tile/dk','9',
        'FP inner/outer','a',
        'tile top/bottom','b',
        'tile inner/outer','c',
        'moon up/down','d',
        'diffpoint best/worst','e',
        '','',
        '','','','',
        'experiment','f',
    ];
    pager.simrd=['SimType',
        // Row 1
        'lcdm+noi','',
        '','',
        // Row 2
//        'lcdm+noi+r=0.2','_simr',
//        '','',
        // Row 3
        'lcdm+noi+dust','_simd'
        ];
    pager.lmax=['lmax',
        '200','200',
        '500','500'
        ];
    pager.cross1=['Cross 1',
        'K_95','1',
        'K_150','2',
        'K_220','3'
        ];
    pager.cross2=['Cross 2',
        'K_95','1',
        'K_150','2',
        'K_220','3'
        ];
    pager.version=['Version',
        'B2016','B2016',
        'B2017','B2017',
        'B2018','B2018',
        ];
    pager.year=['Year',
	'2016','2016',
	'2017','2017',
	'2018','2018',
	];
    pager.sim=['rlz',
	'99','99',
	'199','199',
	'299','299',
	'499','499',
	];
  </script>


